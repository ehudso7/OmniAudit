{
  "version": 2,
  "builds": [
    {
      "src": "api/health.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/skills.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/analyze.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/v1/reviews/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/v1/reviews/stats.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/v1/dashboard/stats.ts",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "api/health.ts"
    },
    {
      "src": "/api/skills",
      "dest": "api/skills.ts"
    },
    {
      "src": "/api/analyze",
      "dest": "api/analyze.ts"
    },
    {
      "src": "/api/v1/reviews/stats",
      "dest": "api/v1/reviews/stats.ts"
    },
    {
      "src": "/api/v1/reviews",
      "dest": "api/v1/reviews/index.ts"
    },
    {
      "src": "/api/v1/dashboard/stats",
      "dest": "api/v1/dashboard/stats.ts"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "python-app:src"
  },
  "build": {
    "env": {
      "PYTHONPATH": "python-app:src"
    }
  }
}
