{
  "name": "@omniaudit/rules-engine",
  "version": "2.0.0",
  "description": "OmniAudit Rules Engine - Pattern matching and rule execution",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "@babel/types": "^7.23.9",
    "zod": "^3.22.4",
    "js-yaml": "^4.1.0",
    "micromatch": "^4.0.5"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.5",
    "@types/js-yaml": "^4.0.9",
    "@types/micromatch": "^4.0.6",
    "@types/node": "^20.11.16",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2",
    "vitest": "^1.2.2"
  },
  "keywords": [
    "rules-engine",
    "pattern-matching",
    "ast",
    "security",
    "code-analysis"
  ],
  "author": "OmniAudit Team",
  "license": "MIT"
}
